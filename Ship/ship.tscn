script/source = "extends Node2D


var airship_speed = 15
const MAX_ALTITUDE = 50
var altitude = MAX_ALTITUDE
const MAX_FUEL = 100
var fuel = MAX_FUEL
@onready var node = get_node(\"AltitudeBar\")
@onready var fuel_node = get_node(\"FuelBar2\")
@onready var propeller = get_node(\"./Balloon/PropellerSprite\")
@onready var fire_sprite = $Fire/FireSprite

# Called when the node enters the scene tree for the first time.
func _ready():
 propeller.play(\"default\", airship_speed)
 fire_sprite.hide()
 $AltitudeBar.max_value = MAX_ALTITUDE
 $FuelBar2.max_value = MAX_FUEL
 update_altitude()
 update_fuel()

# Called every frame. 'delta' is the elapsed time since the previous frame.
#func _process(delta):
	#pass
func update_altitude():
 $AltitudeBar.value = altitude

func update_fuel():
 $FuelBar2.value = fuel


func _on_timer_timeout():
 altitude -= .1
 fuel -= .1

 if altitude == 50 && altitude > 25:
  node.fullAltitude()
 if fuel == 100 && altitude > 50:
  fuel_node.fullfuel()
 if altitude < 25 && altitude > 12.5:
  node.midAltitude()
 if fuel< 50 && fuel > 25:
  fuel_node.midfuel()
 if altitude < 12.5:
  node.lowAltitude()
 if fuel< 25:
  fuel_node.lowfuel()

 update_altitude()
 update_fuel()

func increase():
 altitude += .2
 update_altitude()
 decrease_fuel()

func decrease_fuel():
 fuel -= .2
 update_fuel()
 


"

